      <HintPath>$(MSBuildThisFileDirectory)..\build\UAP10.0\$(Platform)\*.dll</HintPath>
    </Reference>
    <Reference Include="LinphoneSDK" Condition="'$(Platform)' == 'x64'">
      <HintPath>$(MSBuildThisFileDirectory)..\build\UAP10.0\$(Platform)\*.dll</HintPath>
