	mkdir -p ${RPMDIRS} && \
	cp ../${SOURCE_GZIP_FILE} SOURCES && \
	cd SOURCES && \
	gunzip ${SOURCE_GZIP_FILE} && \
	tar xvf ${SOURCE_TAR_FILE} && \
	rm -f ${SOURCE_TAR_FILE} && \
	cd ..  && \
	cp -R SOURCES/${PACKAGE_TMPDIR} BUILD/${PACKAGE_TMPDIR} && \
	rpm ${RPM_DEFINES} ${RPMFLAGS} -ba @PACKAGE@".spec" && \
	cp ${RPM_I386} .. && \
	cp ${RPM_SRC} .. && \
	rm -rf ${RPMDIRS}

